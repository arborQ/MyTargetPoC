{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "jsx": "react",
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "noEmitHelpers": false,
        "noEmit": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "filesGlob": [
        "!./node_modules/**/*",
        "!./wwwroot/jspm_packages/**/*",
        "./typings/**/*.d.ts",
        "./wwwroot/ngApp/**/*.ts",
        "./wwwroot/**/*.tsx"
    ],
    "files": [
        "./typings/angular-ui-router/angular-ui-router.d.ts",
        "./typings/angularjs/angular.d.ts",
        "./typings/jquery/jquery.d.ts",
        "./typings/tsd.d.ts",
        "./wwwroot/ngApp/app.ts",
        "./wwwroot/ngApp/modules/users/controllers/userList.ts",
        "./wwwroot/ngApp/modules/users/users.module.ts",
        "./wwwroot/ngApp/setup.pages.ts"
    ],
    "exclude": []
}
