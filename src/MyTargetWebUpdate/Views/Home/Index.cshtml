<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <title>Just Move</title>
    <link rel="stylesheet" href="~/bower_components/bootstrap/dist/css/bootstrap.min.css" charset="utf-8">
    <link rel="stylesheet" href="~/bower_components/font-awesome/css/font-awesome.min.css" charset="utf-8">
    <link rel="stylesheet" href="~/bower_components/angular-loading-bar/src/loading-bar.css" charset="utf-8">
    <link rel="stylesheet" href="~/bower_components/AngularJS-Toaster/toaster.min.css" charset="utf-8">
    <link rel="stylesheet" href="~/bundles/site.min.css" charset="utf-8">
    <link rel="shortcut icon" type="image/ico" href="/images/logo_16.ico">
</head>
@{
  var appName = "app";
}
<body id="just_move">
    <my-app my-navigations="vm.navigations">
        <div class="loading_placeholder"><div class="loading main"></div></div>
    </my-app>

    <script src="~/common.js" charset="utf-8"></script>

    <environment names="Production">
        <script src='@string.Format("~/bundles/angular/{0}.bundle.sfx.js", appName)' charset="utf-8"></script>
        <script type="text/javascript">
          angular.bootstrap(document, ['app']);
        </script>
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-72004429-1', 'auto');
          ga('send', 'pageview');
        </script>
    </environment>
    <environment names="Development">
        <script src="~/jspm_packages/system.js" charset="utf-8"></script>
        <script src="~/config.js" charset="utf-8"></script>
        <script type="text/javascript">
        System.import('dest/application.angular/@appName').then(function(){
          angular.bootstrap(document, ['app']);
        });
        </script>
    </environment>
</body>
</html>
