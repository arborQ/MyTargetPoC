ui-view
.card.page(ng-if="vm.model.length === 0")
  .card-block
    i Nie ma zmian w magazynie
.card.page(ng-if="vm.model.length !== 0")
  .card-block 
    .row
      .col-lg-12 
        span Aktywnosc 
  .list-group.list-group-flush
    a.list-group-item(ng-repeat="product in vm.model")
      span 
        span {{product.ProductName}} 
      span.label.label-default.label-pill.pull-right(ng-class="{ 'label-success' : product.Value > 0 }") {{product.Value}} szt.

 