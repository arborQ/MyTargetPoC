{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "jsx": "react",
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "noEmitHelpers": true,
        "noEmit": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "filesGlob": [
        "!./node_modules/**/*",
        "!./wwwroot/jspm_packages/**/*",
        "./typings/**/*.d.ts",
        "./wwwroot/ngApp/**/*.d.ts",
        "./wwwroot/ngApp/**/*.ts",
        "./wwwroot/**/*.tsx"
    ],
    "files": [
        "./typings/angular-ui-router/angular-ui-router.d.ts",
        "./typings/angularjs/angular-resource.d.ts",
        "./typings/angularjs/angular.d.ts",
        "./typings/history/history.d.ts",
        "./typings/jquery/jquery.d.ts",
        "./typings/material-ui/material-ui.d.ts",
        "./typings/react-router/history.d.ts",
        "./typings/react-router/react-router.d.ts",
        "./typings/react/react-dom.d.ts",
        "./typings/react/react.d.ts",
        "./typings/tsd.d.ts",
        "./wwwroot/ngApp/modules/interfaces/IEntity.d.ts",
        "./wwwroot/ngApp/modules/interfaces/ui/ISelectable.d.ts",
        "./wwwroot/ngApp/modules/products/interfaces/product.d.ts",
        "./wwwroot/ngApp/modules/products/interfaces/search.d.ts",
        "./wwwroot/ngApp/modules/users/interfaces/user.d.ts",
        "./wwwroot/ngApp/app.ts",
        "./wwwroot/ngApp/modules/products/controllers/productCreate.ts",
        "./wwwroot/ngApp/modules/products/controllers/productEdit.ts",
        "./wwwroot/ngApp/modules/products/controllers/productList.ts",
        "./wwwroot/ngApp/modules/products/products.module.ts",
        "./wwwroot/ngApp/modules/setup/config.ts",
        "./wwwroot/ngApp/modules/setup/controller.ts",
        "./wwwroot/ngApp/modules/setup/i18n.ts",
        "./wwwroot/ngApp/modules/setup/pages.ts",
        "./wwwroot/ngApp/modules/setup/viewHelper.ts",
        "./wwwroot/ngApp/modules/users/controllers/userDisplay.ts",
        "./wwwroot/ngApp/modules/users/controllers/userList.ts",
        "./wwwroot/ngApp/modules/users/users.module.ts",
        "./wwwroot/rApp/app.tsx",
        "./wwwroot/rApp/components/products/productList.tsx",
        "./wwwroot/rApp/components/products/views/productListView.tsx",
        "./wwwroot/rApp/components/shared/_master.tsx"
    ],
    "exclude": [],
    "atom": {
        "rewriteTsconfig": true
    }
}
